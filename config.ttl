# Skosmos main configuration
:config a skosmos:Configuration ;\t
 #skosmos:sparqlEndpoint <http://localhost:3030> ;
	skosmos:sparqlDialect "Generic" ;
	skosmos:sparqlCollationEnabled true ;
	skosmos:sparqlTimeout 20 ;\t
     skosmos:httpTimeout 5 ;
	skosmos:serviceName "UN Linked Data" ;
	skosmos:baseHref "https://metadata.un.org/skosmos/" ;
	skosmos:languages (\t
     [ rdfs:label "ar" ; rdf:value "ar_EG.utf8" ]\t
     [ rdfs:label "zh" ; rdf:value "zh_CN.utf8" ]\t
     [ rdfs:label "en" ; rdf:value "en_GB.utf8" ]\t
     [ rdfs:label "fr" ; rdf:value "fr_FR.utf8" ]\t
     [ rdfs:label "ru" ; rdf:value "ru_RU.utf8" ]\t
     [ rdfs:label "es" ; rdf:value "es_ES.utf8" ]\t
     ) ;
	skosmos:searchResultsSize 20 ;
	skosmos:transitiveLimit 1000 ;
	skosmos:logCaughtExceptions true ;\t
     skosmos:logBrowserConsole true ;\t
     skosmos:templateCache "/tmp/skosmos-template-cache" ;\t
     skosmos:feedbackAddress "" ;\t
     skosmos:feedbackSender "" ;\t
     skosmos:feedbackEnvelopeSender "" ;\t
     skosmos:uiLanguageDropdown false ;\t
     skosmos:uiHoneypotEnabled true ;\t
     skosmos:uiHoneypotTime 5 ;\t
     skosmos:globalPlugins () .

:unbist a skosmos:Vocabulary, void:Dataset ;\t
 dc:title "UNBIS Thesaurus Remote" ;\t
 skosmos:shortName "UNBIST Remote" ;\t
 dc:subject :cat_general ;\t
 void:uriSpace "http://metadata.un.org/thesaurus/" ;\t
 skosmos:language "ar", "zh", "en", "fr", "ru", "es" ;\t
 skosmos:defaultLanguage "en" ;\t
 skosmos:fallbackLanguages "ar", "zh", "fr", "ru", "es" ;\t
 skosmos:explicitLanguageTags true ;\t
 skosmos:showTopConcepts true ;\t
 skosmos:fullAlphabeticalIndex true ;\t
 skosmos:groupClass eu:Domain ;\t
 skosmos:sortByNotation true ;\t
 skosmos:showNotation true ;\t
 skosmos:conceptSchemesInHierarchy true ;\t
 skosmos:mainConceptScheme <http://metadata.un.org/thesaurus/00> ;\t
 void:sparqlEndpoint <https://metadata.un.org/graphdb/repositories/UNBIST_core> ;\t
 skosmos:sparqlDialect "Generic" .

